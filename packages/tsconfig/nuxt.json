{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Nuxt",
  "extends": "@total-typescript/tsconfig/bundler/dom",
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Node10",
    "sourceMap": true,
    "noImplicitAny": false,
    "allowImportingTsExtensions": true,
    "verbatimModuleSyntax": false
  },
  "vueCompilerOptions": {
    "target": 2.7
  }
}
